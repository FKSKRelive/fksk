{
    "name": "fkskev/fksk",
    "description": "FKSK ev web site",
    "keywords": ["kamerun", "karlsruhe", "student", "deutschland"],
    "homepage": "http://www.fksk-kamer-ka.de/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/FKSKRelive/fksk/issues",
